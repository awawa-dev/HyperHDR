IDI_ICON1 ICON DISCARDABLE "${WIN_RC_ICON_PATH}"
1 VERSIONINFO
 FILEVERSION ${HYPERHDR_VERSION_MAJOR},${HYPERHDR_VERSION_MINOR},${HYPERHDR_VERSION_PATCH},0
 PRODUCTVERSION ${HYPERHDR_VERSION_MAJOR},${HYPERHDR_VERSION_MINOR},${HYPERHDR_VERSION_PATCH},0
 FILEFLAGSMASK 0x17L
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x4L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName", "HyperHDR-project"
            VALUE "Comments", "https://github.com/awawa-dev/HyperHDR"
            VALUE "FileDescription", "HyperHDR Ambient Lighting"
            VALUE "FileVersion", "${HYPERHDR_VERSION}"
            VALUE "InternalName", "HyperHdr"
            VALUE "LegalCopyright", "MIT"
            VALUE "OriginalFilename", "${BINARY_NAME}"
            VALUE "ProductName", "HyperHDR Ambient Lighting"
            VALUE "ProductVersion", "${HYPERHDR_VERSION}"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
